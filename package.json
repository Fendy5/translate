{
  "name": "translate",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "start": "react-native start --experimental-debugger",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15 Pro'",
    "release:patch": "npm version patch && npm run updateVersion",
    "release:minor": "npm version minor && npm run updateVersion",
    "release:major": "npm version major && npm run updateVersion",
    "updateVersion": "npm run postversion && npm run build:android && curl -X GET https://translate.fendy5.cn/api/v1/update_version?v=$npm_package_version",
    "build:android": "cd android && ./gradlew assembleRelease",
    "postversion": "react-native-version",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/material-bottom-tabs": "^6.2.28",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/native-stack": "^6.9.25",
    "appcenter": "^5.0.1",
    "appcenter-analytics": "^5.0.1",
    "appcenter-crashes": "^5.0.1",
    "axios": "^1.6.8",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-asset": "^2.1.1",
    "react-native-date-picker": "^5.0.2",
    "react-native-device-info": "^10.13.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-sound": "^0.11.2",
    "react-native-svg": "^15.1.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-vector-icons": "^10.0.3",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
